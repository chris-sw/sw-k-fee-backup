{
  "name": "gulp-boilerplate",
  "version": "1.0.0",
  "homepage": "http://mzdr.it",
  "description": "Gulp Boilerplate",
  "moduleType": [
    "node"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "normalize-css": "latest",
    "picturefill": "latest",
    "fontawesome": "~4.3.0",
    "slick.js": "~1.4.1",
    "jquery": "~2.1.3",
    "matchHeight": "~0.5.2"
  },
  "overrides": {
    "slick.js": {
      "main": [
        "slick/slick.min.js",
        "slick/slick.css"
      ]
    }
  }
}
