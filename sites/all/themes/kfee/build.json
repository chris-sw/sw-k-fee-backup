{
    "amd": true,
    "src":  "./src",
    "dest": ".",
    "directories": {
        "fonts": "fonts",
        "images": "images",
        "scripts": "js",
        "styles": "css"
    },
    "files": {
        "fonts": ["**/*.{woff2,woff,svg,eot,ttf,otf}"],
        "images": ["**/*.{png,jpg,jpeg,gif,svg}"],
        "scripts": ["**/*.js"],
        "styles": ["**/*.{sass,css}"]
    },
    "browserSync": {
        "proxy": "sowhat.dev/kfee-styleguide/build",
        "watchForChange": "./build/**/*.{html,php}"
    }
}
